<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        tools:context=".fragment.ModuleTestFragment">

        <fragment
            android:id="@+id/serial"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:name="com.cn.test.fragment.Serial_Operation" />

        <fragment
            android:id="@+id/data_show"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:name="com.cn.test.fragment.Data_Show" />

        <fragment
            android:id="@+id/base_setting"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:name="com.cn.test.fragment.Base_Setting" />

        <fragment
            android:id="@+id/data_operation"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:name="com.cn.test.fragment.Data_Operation" />

    </LinearLayout>
</layout>